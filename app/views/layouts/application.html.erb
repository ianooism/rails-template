<!DOCTYPE html>
<html>
  <head>
    <title>Helpdesk</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |name, message| %>
      <%= content_tag :div, message, class: name %>
    <% end %>
    
    <div><%= link_to 'Home', root_path %></div>
    
    <%= yield %>
  </body>
</html>
